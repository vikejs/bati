{
  "name": "@batijs/react-sentry",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "bati-compile-boilerplate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@batijs/compile": "workspace:*",
    "@sentry/react": "^10.10.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@batijs/core": "workspace:*"
  },
  "files": [
    "dist/"
  ],
  "bati": {
    "if": {
      "flag": {
        "$all": [
          "react",
          "sentry"
        ]
      }
    }
  },
  "exports": {
    "./sentry.browser.config": {
      "types": "./dist/types/sentry.browser.config.d.ts"
    },
    "./pages/sentry/+Page": {
      "types": "./dist/types/pages/sentry/+Page.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "sentry.browser.config": [
        "./dist/types/sentry.browser.config.d.ts"
      ],
      "pages/sentry/+Page": [
        "./dist/types/pages/sentry/+Page.d.ts"
      ]
    }
  }
}
